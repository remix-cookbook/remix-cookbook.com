// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Favorite {
  id              String   @id @default(uuid())
  createdAt       DateTime @default(now())
  userId          String
  articleTitle    String
  articleSlug     String
}