// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Bookmark {
  id              String   @id @default(uuid())
  createdAt       DateTime @default(now())
  userId          String
  postTitle       String
  postSlug        String
}
